<PixelModules>
  
  <!-- ######## Updated module dimensions after pixel TDR ###### -->
  <!--  These modules are used to implement the inner edges as active-->
  
  <FrontEndChip>
      <name> RD53_20x19 </name>
      <chipLength>20.0</chipLength>
      <chipWidth>19.2</chipWidth>
      <pitchEta>0.050</pitchEta>
      <pitchPhi>0.050</pitchPhi>
      <pitchEtaLong>0.0</pitchEtaLong>
      <pitchPhiLong>0.0</pitchPhiLong>
      <pitchEtaEnd>0.0</pitchEtaEnd>
      <pitchPhiEnd>0.0</pitchPhiEnd>
      <columns>400</columns>
      <rows>384</rows>
      <nPhiLongPerSide>0</nPhiLongPerSide>
      <nEtaLongPerSide>0</nEtaLongPerSide>
      <nPhiEndPerSide>0</nPhiEndPerSide>
      <nEtaEndPerSide>0</nEtaEndPerSide>
      <chipGap>0.0</chipGap>
  </FrontEndChip>

  <!-- rot means rotation -->  
  <FrontEndChip>
      <name> RD53_20x19_rot </name>
      <chipLength>19.2</chipLength>
      <chipWidth>20.0</chipWidth>
      <pitchEta>0.050</pitchEta>
      <pitchPhi>0.050</pitchPhi>
      <pitchEtaLong>0.0</pitchEtaLong>
      <pitchPhiLong>0.0</pitchPhiLong>
      <pitchEtaEnd>0.0</pitchEtaEnd>
      <pitchPhiEnd>0.0</pitchPhiEnd>
      <columns>384</columns>
      <rows>400</rows>
      <nPhiLongPerSide>0</nPhiLongPerSide>
      <nEtaLongPerSide>0</nEtaLongPerSide>
      <nPhiEndPerSide>0</nPhiEndPerSide>
      <nEtaEndPerSide>0</nEtaEndPerSide>
      <chipGap>0.0</chipGap>
  </FrontEndChip>
  
  <FrontEndChip>
      <name> RD53_20x19_25x100 </name>
      <chipLength>20.0</chipLength>
      <chipWidth>19.2</chipWidth>
      <pitchEta>0.100</pitchEta>
      <pitchPhi>0.025</pitchPhi>
      <pitchEtaLong>0.0</pitchEtaLong>
      <pitchPhiLong>0.0</pitchPhiLong>
      <pitchEtaEnd>0.0</pitchEtaEnd>
      <pitchPhiEnd>0.0</pitchPhiEnd>
      <columns>200</columns>
      <rows>768</rows>
      <nPhiLongPerSide>0</nPhiLongPerSide>
      <nEtaLongPerSide>0</nEtaLongPerSide>
      <nPhiEndPerSide>0</nPhiEndPerSide>
      <nEtaEndPerSide>0</nEtaEndPerSide>
      <chipGap>0.0</chipGap>
  </FrontEndChip>
  
  <!-- quad modules have different chip length and width due to the long pixels
       in the interchip region -->
  <FrontEndChip>
      <name> RD53_20x19_Quad </name>
      <chipLength>20.1</chipLength>
      <chipWidth>19.3</chipWidth>
      <pitchEta>0.050</pitchEta>
      <pitchPhi>0.050</pitchPhi>
      <pitchEtaLong>0.100</pitchEtaLong>
      <pitchPhiLong>0.100</pitchPhiLong>
      <pitchEtaEnd>0.0</pitchEtaEnd>
      <pitchPhiEnd>0.0</pitchPhiEnd>
      <columns>400</columns>
      <rows>384</rows>
      <nPhiLongPerSide>2</nPhiLongPerSide>
      <nEtaLongPerSide>2</nEtaLongPerSide>
      <nPhiEndPerSide>0</nPhiEndPerSide>
      <nEtaEndPerSide>0</nEtaEndPerSide>
      <chipGap>0.0</chipGap>
  </FrontEndChip>
  
  <!-- ######## ITK modules  ########### -->
  
  <!-- ### 50 um x 50 um pixels ### -->
  <!-- ##### Updated chip dimensions after pixel TDR ###### -->
  
  <Module>
      <moduleName> SingleChip_RD53_3D</moduleName>
      <chipName> RD53_20x19 </chipName>
      <lengthInChips>1</lengthInChips>
      <widthMaxInChips>1</widthMaxInChips>
      <sensorThickness>0.150</sensorThickness>
      <chipThickness>0.150</chipThickness>
      <hybridThickness>0.1525</hybridThickness>
      <support3DThickness>0.100</support3DThickness>
      <Edges>
          <wide>0.150</wide>
          <narrow>0.150</narrow>
          <inlength>0.150</inlength>
        </Edges>
  </Module>
  
  <Module>
      <moduleName> SingleChip_RD53_3D_rot</moduleName>
      <chipName> RD53_20x19_rot </chipName>
      <lengthInChips>1</lengthInChips>
      <widthMaxInChips>1</widthMaxInChips>
