  </Module>
  
  <Module>
      <moduleName>QuadChip_RD53</moduleName>
      <chipName> RD53_20x19_Quad </chipName>
      <lengthInChips>2</lengthInChips>
      <widthMaxInChips>2</widthMaxInChips>
      <sensorThickness>0.150</sensorThickness>
      <chipThickness>0.150</chipThickness>
